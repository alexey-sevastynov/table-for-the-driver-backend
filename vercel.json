{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "nodejs14.x",
      "events": [
        {
          "schedule": "cron(*/1 * * * *)",
          "name": "check-mongodb"
        }
      ]
    }
  }
}
